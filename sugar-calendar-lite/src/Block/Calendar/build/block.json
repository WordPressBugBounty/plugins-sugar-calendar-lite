{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "sugar-calendar/block",
  "version": "1.0.1",
  "title": "Events Calendar",
  "category": "sugar-calendar",
  "description": "Sugar Calendar block for displaying the events calendar.",
  "example": {},
  "attributes": {
    "calendarId": {
      "type": "string",
      "default": ""
    },
    "calendars": {
      "type": "array",
      "default": []
    },
    "tags": {
      "type": "array",
      "default": []
    },
    "venues": {
      "type": "array",
      "default": []
    },
    "speakers": {
      "type": "array",
      "default": []
    },
    "display": {
      "type": "string",
      "default": "month"
    },
    "showBlockHeader": {
      "type": "boolean",
      "default": true
    },
    "allowUserChangeDisplay": {
      "type": "boolean",
      "default": true
    },
    "showFilters": {
      "type": "boolean",
      "default": true
    },
    "showSearch": {
      "type": "boolean",
      "default": true
    },
    "appearance": {
      "type": "string",
      "default": "light"
    },
    "accentColor": {
      "type": "string",
      "default": "#5685BD"
    }
  },
  "textdomain": "sugar-calendar-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "script": [
    "sugar-calendar-js"
  ]
}